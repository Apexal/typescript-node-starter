{
  "name": "typescript-node-starter",
  "version": "0.1.0",
  "private": true,
  "description": "Starter for a Node project using TypeScript.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "lint": "tsc --noEmit && eslint . --ext .js,.ts --quiet --fix",
    "build": "npm run lint && npm run build-ts",
    "build-ts": "tsc",
    "test": "jest --forceExit --coverage --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/late-project/coursework-api.git"
  },
  "keywords": [
    "school"
  ],
  "author": "Frank Matranga <frank@matranga.family>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Apexal/typescript-node-starter/issues"
  },
  "homepage": "https://github.com/Apexal/typescript-node-starter#readme",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "winston": "^3.3.3"
  }
}
